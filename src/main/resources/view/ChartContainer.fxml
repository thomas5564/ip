<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<fx:root id="root" prefHeight="800.0" prefWidth="100.0" stylesheets="@chart-container.css" type="VBox" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox prefHeight="200.0" prefWidth="100.0">
            <children>
                <Label text="This week's performance" />
                <VBox fx:id="squaresContainer" prefHeight="200.0" prefWidth="100.0" />
            </children>
        </VBox>
        <VBox prefHeight="200.0" prefWidth="100.0">
            <children>
                <Label text="Last week's performance" />
                <VBox fx:id="pieContainer" prefHeight="200.0" prefWidth="100.0" />
            </children>
        </VBox>
        <VBox prefHeight="600.0" prefWidth="100.0">
            <children>
                <Label text="Weekly finish rate for previous 4 weeks" />
                <VBox fx:id="barContainer" prefHeight="600.0" prefWidth="100.0" VBox.vgrow="ALWAYS"/>
            </children>
        </VBox>
    </children>
</fx:root>
